<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:five="http://namespaces.zope.org/five"
    xmlns:i18n="http://namespaces.zope.org/i18n"
    xmlns:genericsetup="http://namespaces.zope.org/genericsetup"
    i18n_domain="plone.app.robotframework">

  <include package="Products.GenericSetup" />

  <genericsetup:registerProfile
      name="content"
      title="Plone Act content"
      directory="profiles/content"
      description="Installs content inside Plone site"
      provides="Products.GenericSetup.interfaces.EXTENSION"
      />

  <genericsetup:importStep
      name="plone.app.robotframework-content"
      title="plone.app.robotframework: setup content"
      description="Setup content to be used by tests"
      handler="plone.app.robotframework.setuphandlers.setupContent">
  </genericsetup:importStep>

</configure>
